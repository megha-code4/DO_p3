#!/usr/bin/env groovy
pipeline{
  agent any
tools {
    maven 'Maven' // This should match the name you provided in Global Tool Configuration
}
  stages{

    stage('validate'){
      steps{
        script{
          bat "mvn validate"
        }
      }
    }
    stage('compile'){
      steps{
        script{
          bat 'mvn compile'
        }
      }
    }
    stage('test'){
      steps{
        script{
          bat "mvn test"
        }
      }
    }
  }
}
